@page "/plan"

@using Microsoft.EntityFrameworkCore
@using WorkoutPlanner.Context
@using WorkoutPlanner.Model



<h3>Plan</h3>

<table class="table">
    <thead>
        <tr>
            <th>Workout Name</th>
            <th>Category</th>
            <th>Duration (mins)</th>
            <th>Reps</th>
            <th>Sets</th>
            <th>Weight (kg)</th>
            <th>Rest Time (secs)</th>
            <th>Exercise Info</th>
        </tr>
    </thead>
    <tbody>
       
        
            <tr>
                <td>@Workout.ExerciseName</td>
                
            </tr>
        
    </tbody>
</table>

@code {

    [Parameter]
    public Workout Workout { get; set; } = new Workout();
    private List<Workout> workouts;

}
